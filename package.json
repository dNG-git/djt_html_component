{
    "name": "djt-html-riot-tag",
    "license": "MPL-2.0",
    "description": "'djt-html-riot-tag' adds additional common methods to Riot.js tags.",
    "homepage": "https://www.direct-netware.de/redirect?djt;html;riot_tag",
    "version": "1.0.0",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/dNG-git/djt_html_riot_tag.git"
    },
    "main": "./lib/cjs/module.js",
    "module": "./lib/module.js",
    "types": "./lib/module.d.ts",
    "scripts": {
        "install-clean": "npm install --no-save",
        "update-clean": "npm update --no-save",
        "build": "npm run build-lib && npm run build-dist",
        "build-dist": "npm run build-dist:es5 && npm run build-dist:module",
        "build-dist:es5": "./node_modules/.bin/rollup -c rollup.config-es5.js",
        "build-dist:module": "./node_modules/.bin/rollup -c",
        "build-lib": "npm run build-lib:es6 && npm run build-lib:cjs",
        "build-lib:es6": "./node_modules/.bin/tsc",
        "build-lib:cjs": "./node_modules/.bin/tsc -p tsconfig.cjs.json",
        "prepare": "npm run build-lib"
    },
    "dependencies": {
        "djt-promised-require": "git://github.com/dNG-git/djt_promised_require.git#semver:^1.2.0",
        "riot": "^3.13.1",
        "zepto": "^1.2.0"
    },
    "devDependencies": {
        "rollup": "^0.67.3",
        "rollup-plugin-commonjs": "^9.2.0",
        "rollup-plugin-node-resolve": "^3.4.0",
        "rollup-plugin-terser": "^3.0.0",
        "rollup-plugin-typescript2": "^0.18.0",
        "tslint": "^5.11.0",
        "typescript": "^3.1.6",
        "@types/riot": "^3.6.1",
        "@types/zepto": "^1.0.30"
    }
}
